			{
				"before": ["j", "k"],
				"after": ["<Esc>"]
			},
			{
				"before": ["ח", "ל"],
				"after": ["<Esc>"]
			}
	],
	"vim.normalModeKeyBindingsNonRecursive": [
			{
					"before": ["leader", "w"],
					"commands": [
							"workbench.action.files.save",
					]
			},
			{
					"before": ["leader", "'"],
					"commands": [
							"workbench.action.files.save",
					]
			},
			{
					"before": ["g", "b"],
					"commands": [
							"workbench.action.quickOpenNavigateNextInEditorPicker",
					]
			},
			{
					"before": ["leader", "b"],
					"commands": [
							"workbench.action.quickOpen",
					]
			},
			{
				"before":["leader", "<Space>"],
				"commands": [
						":nohl"
				]
		}
